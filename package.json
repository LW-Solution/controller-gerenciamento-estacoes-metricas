      "typeorm": "0.3.20"
   },
   "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/node": "^20.12.7",
      "@types/swagger-jsdoc": "^6.0.4",
      "@types/swagger-ui-express": "^4.1.6",
      "cross-env": "^7.0.3",
      "dotenv": "^16.4.5",
      "swagger-jsdoc": "^6.2.8",
